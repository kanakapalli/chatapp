<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sulphuricacid.anuragfamilyapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ak_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ak_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sulphuricacid.anuragfamilyapp.mainscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sulphuricacid.anuragfamilyapp.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sulphuricacid.anuragfamilyapp.login_screen" />
        <activity android:name="com.sulphuricacid.anuragfamilyapp.forgotpassword" />
        <activity android:name="com.sulphuricacid.anuragfamilyapp.signIn" />
        <activity android:name="com.sulphuricacid.anuragfamilyapp.new_message_activity">
            <meta-data android:name="andriod.support.PARENT_ACTIVITY"
                android:value=".mainscreen"/>
        </activity>
    </application>

</manifest>